// multiplication d'un nombre

#include <stdio.h>

int multiplication(int x){
    for(int i=0;i<=10;i++){
        printf("%d*%d=%d\n",x,i,x*i);
    }
}

int main() {
    // Write C code here
    int x;
    
    printf("ecrire un nombre :");
    scanf("%d",&x);
    
    multiplication(x);
    return 0;
}
