// celsius to fahrenheit et fahrenheit to celsius
#include <stdio.h>

int celsius_to_fahrenheit(int x){
    x=x*9/5+32;
    printf("%d",x);
}

int fahrenheit_to_celsius(int x){
    x=(x-32)*5/9;
    printf("%d",x);
}


int main() {
    int choix;
    
    printf("Choisire le nombre de votre Formules de conversion\n");
    printf("1-celsius to fahrenheit\n");
    printf("2-fahrenheit to celsius\n");
    scanf("%d",&choix);
    
    if(choix==1){
        int c=0;
        
        printf("Ecrire la valeur en Celsius : ");
        scanf("%d",&c);
        celsius_to_fahrenheit(c);
        
    }else if(choix==2){
        int f=0;
        
        printf("Ecrire la valeur en fahrenheit : ");
        scanf("%d",&f);
        celsius_to_fahrenheit(f);
    }
    
    return 0;
}
