// max

#include <stdio.h>


int max(int list[],int tail){
    int max=0;
    for(int i=0;i<tail;i++){
        if(list[i]>max){
            max=list[i];
        }
    }return max;
}

int main() {
    
    
    int x,t;
    
    printf("ecrire la tail du list :");
    scanf("%d",&x);
    
    int list[x];
    
    for(int i=0;i<x;i++){
        printf("ecrire la valeur #%d =>",i);
        scanf("%d",&list[i]);
    }
    
    t=max(list,x);
    printf("le plus grand nombre de list est :%d",t); 
    
    return 0;
}

