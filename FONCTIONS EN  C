// cherche un nnombre dans un list
#include <stdio.h>

 int chercher_valeur(int list[],int valeur,int tail){
     for(int i=0;i<=tail;i++){
         if(list[i]==valeur){
             return i;
         }
     }
 }

int main() {
    // Write C code here
    int n,c,t;
    
    printf("ecrire la valeur du list :");
    scanf("%d",&n);
    
    int list[n];
    
    for(int i=0;i<n;i++){
    printf("ecrire le nombre #%d =>",i);
    scanf("%d",&list[i]);
    }
    
    printf("Entrer la valeur a chercher: ");
    scanf("%d",&c);
    
    t=chercher_valeur(list,c,n);
    
    printf("le nombre %d est dans la cellule %d ",c,t);
    
    return 0;
}
