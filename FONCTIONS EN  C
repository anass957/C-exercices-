// les nombre pairs

#include <stdio.h>

int trouver_pairs(int tail,int list1[],int list[]){
    int j=0;
    for(int i=0;i<tail;i++){
        if(list1[i]%2==0){
            list[j]=list1[i];
            j++;
        }
    }return  j;
}



int main() {
    // Write C code here
    int tail;
    
    printf("ecrire la tail du list :");
    scanf("%d",&tail);
    
    int list[tail];
    int pairs[tail];
    
    printf("remplire la list :\n");
    for(int i=0;i<tail;i++){
        printf("%d==>",i);
        scanf("%d",&list[i]);
    }
    
    int p= trouver_pairs(tail, list, pairs);
    printf("Les nombres pairs sont : ");
    for (int i = 0; i < p; i++) {
        printf("%d ", pairs[i]);
    }
    
    return 0;
}
