 //le plus grand nombre
#include <stdio.h>

int max(int x,int y){
    int max;
    if(x>y){
        max=x;
    }else if(x<y){
        max=y;
    }
    printf("le plus grand nombre est :%d",max);
}

int main() {
    
    int a,b;
    
    printf("ecrire deux nombre :");
    scanf("%d %d",&a,&b);
    max(a,b);
   
    return 0;
}
