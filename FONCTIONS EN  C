// fusionner deux tableaux 

#include <stdio.h>

 int fusionner_tableaux(int tail,int list1[],int list2[],int list3[]){
     
     for(int i=0;i<tail;i++){
         list3[i]=list1[i];
     }
     for(int i=0;i<tail;i++){
         list3[i+tail]=list2[i];
     }
     return list3[tail];
 }



int main() {
    // Write C code here
    int tail;
    
    printf("ecrire la taille des deux tableaux :");
    scanf("%d",&tail);
    
    int list1[tail];
    int list2[tail];
    
    for(int i=0;i<tail;i++){
        printf("remplire le premier tableau :");
        printf("#%d==>",i);
        scanf("%d",&list1[i]);
    }
    
    for(int i=0;i<tail;i++){
        printf("remplire le deuxieme tableau :");
        printf("#%d==>",i);
        scanf("%d",&list2[i]);
    }
    
    int list3[tail * 2];
    
    fusionner_tableaux(tail, list1, list2, list3);
    printf("Tableau fusionné : ");
   
    for(int i=0;i<tail*2;i++) {
        printf("%d ", list3[i]);
    }
    

    return 0;
}
