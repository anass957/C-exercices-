// dimension du rectangle 

#include <stdio.h>
int perimetre_rectangle(int x,int y){
    int p;
    p=2*(x+y);
    printf("le perimetre du rectangle est %d\n",p);
}

int Surface_rectangle(int x,int y){
    int s;
    s=x*y;
    printf("la surface du rectangle est %d\n",s);
}

int main() {
    
    int lo,la;
    
    printf("ecrire le longueur et le largeur de rectangle :");
    scanf("%d %d",&lo,&la);
    
    Surface_rectangle(lo,la);
    perimetre_rectangle(lo,la);
    
    
    
    return 0;
}
