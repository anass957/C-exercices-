// calcule puissance

#include <stdio.h>

int puissance(int b,int e){
    int t=1;
    
    for(int i=0;i<e;i++){
        t=b*t;
    }
    //printf("%d",t);
return t;
}

int main() {
    int b,e;
    
    printf("ecrire la base et l'exposant :");
    scanf("%d %d",&b,&e);
    puissance(b,e);
    
    float v=puissance(r, 3)+ puissance(r1, 2);
    
    
    return 0;
}


