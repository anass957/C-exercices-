//PGCD 

#include <stdio.h>

int pgcd(int a, int b){
    int p, min;
    if(a>b){
        min=b;
    }else {min=a;}
    
    
    for(int i=1;i<=min;i++){
        if(a % i ==0 && b % i ==0 ){
            p = i;
        }
    }
    
    return p;
    
}

int main() {
    
    //Les diviseurs de 12 sont 1, 2, 3, 4, 6 et 12
    int a,b,t;
    
    
    printf("ecrire les deux nombre :");
    scanf("%d %d",&a,&b);
    
    int list1[a];
    
  
    
    t=pgcd(a,b);
    printf("%d",t);
    
    return 0;
}


