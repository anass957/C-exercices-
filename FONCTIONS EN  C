// compter_occurrences

#include <stdio.h>

   int compter_occurrences(int list[],int x,int valeur){
       
       int t=0;
       for(int i=0;i<x;i++){
           if(list[i]==valeur){
               t++;
           }
       }return t;
   }

int main() {
    // Write C code here
    int x,a,v;
    
    printf("ecrire la taile des list :");
    scanf("%d",&x);

   int list[x];

   for(int i=0;i<x;i++){
       printf("ecrire la valeur de #%d =>",i);
       scanf("%d",&list[i]);
   }
   
   printf("ecrire la valeur que vous voulez voir combien il se répeter :");
   scanf("%d",&a);

   v=compter_occurrences(list,x,a);
  
   printf("le nombre se répter %d fois",v);

    return 0;
}
