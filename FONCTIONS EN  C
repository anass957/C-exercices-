//somme des nombre

#include <stdio.h>

  int calcule_nombre(int list[], int x){
   int t=0;
     
    for(int i=0;i<x;i++){
         t=t+list[i];
    }
    return t; 
  }

int main() {
    int x,t=0;
    
    printf("ecrire combien des nombre dans votre nombre:");
    scanf("%d",&x);
    
    int list[x];
    
    for(int i=0;i<x;i++){
    printf("ecrire le nombre :");
    scanf("%d",&list[i]);
    }
    
    printf("votre nombre est :");
     for(int i=0;i<x;i++){
     printf("%d",list[i]);
     }
     
     t=calcule_nombre(list,x);
     printf("\nest la somme de votre nombre est :%d",t);  
     

    
    
    return 0;
}

