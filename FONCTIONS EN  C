
//calcule factorielle 
#include <stdio.h>

    int factorielle(int n){
        int t=1;
        for(int i=1;i<=n;i++){
            t=t*i;
        }
        return t;
    }

int main() {
    int x,s;
    
    printf("ecrire un nombre :");
    scanf("%d",&x);
    
    s=factorielle(x);
    
    printf("le factoriel de %d est %d :",x,s);

    return 0;
}
